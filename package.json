{
  "name": "solve-it",
  "version": "1.0.0",
  "description": "Platform for solving coding problems for compensation",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "seed": "node script/seed.js",
    "start": "node server",
    "start:dev": "npm run build:dev & npm run start-server",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start:dev:seed": "SEED=true npm run start:dev",
    "start-server": "nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV=test JWT=a_secret mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" --require @babel/register --require esm ",
    "test:dev": "npm run test -- --watch",
    "test:dev:models": "npm run test:dev -- --grep=model",
    "test:dev:routes": "npm run test:dev -- --grep=route",
    "docker-build": "cd ./server/docker && docker build -t solve-it/node-sandbox-app ."
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/polyfill": "^7.12.1",
    "@monaco-editor/react": "^4.1.3",
    "@uiw/react-md-editor": "^3.0.8",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.3",
    "dateformat": "^4.5.1",
    "dockerode": "^3.3.0",
    "dotenv": "^9.0.2",
    "express": "^4.16.4",
    "history": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "pg": "^8.5.1",
    "react-confirm-alert": "^2.7.0",
    "react-mde": "^11.5.0",
    "react-search-field": "^1.2.1",
    "react-toastify": "^7.0.4",
    "sequelize": "^6.3.5",
    "showdown": "^1.9.1",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.16.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "css-loader": "^5.2.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "esm": "^3.2.25",
    "mocha": "^6.2.3",
    "nodemon": "^1.19.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.1",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.4.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "style-loader": "^2.0.0",
    "supertest": "^4.0.2",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1"
  }
